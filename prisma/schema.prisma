// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../static/css.sqlite"
}

model css {
  name         String
  category     String
  parent       String
  browser      String
  version      Float
  link         String?
  description  String?

  @@id([name, category, parent, browser, version])
  @@index([browser, version])
}

model versions {
  browser       String
  version       Float
  release_date  DateTime

  @@id([browser, version])
}
